  keywords = {Ageing,Colour,Evoked-potential,Motion,Spatio-temporal frequency},
  pages = {3557--3566},
  file = {FIORENTINI et al_1996_Visual Ageing.pdf:/Users/jan/Documents/Zotero/storage/S3X7UKMG/FIORENTINI et al_1996_Visual Ageing.pdf:application/pdf;ScienceDirect Snapshot:/Users/jan/Documents/Zotero/storage/RVM22GNA/0042698996000326.html:text/html},
