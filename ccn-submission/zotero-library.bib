  journal = {Journal of Neuroscience},
  author = {Anderson, Elaine J. and Tibber, Marc S. and Schwarzkopf, D. Sam and Shergill, Sukhwinder S. and Fernandez-Egea, Emilio and Rees, Geraint and Dakin, Steven C.},
  month = feb,
  year = {2017},
  pages = {3620--15},
  file = {Anderson et al_2016_Visual population receptive fields in people with schizophrenia have reduced.pdf:/Users/jan/Documents/Zotero/storage/IZG8EMNW/Anderson et al_2016_Visual population receptive fields in people with schizophrenia have reduced.pdf:application/pdf;Snapshot:/Users/jan/Documents/Zotero/storage/CF6XJ642/JNEUROSCI.3620-15.html:text/html},
