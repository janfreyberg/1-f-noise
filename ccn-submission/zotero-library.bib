  year = {2012},
  keywords = {anisotropy,contours,crowding,model selection,natural scenes,Peripheral vision,Pooling,reverse correlation,texture},
  file = {Snapshot:/Users/jan/Documents/Zotero/storage/VXW2H66V/6.html:text/html},
