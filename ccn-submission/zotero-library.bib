@article{kraehenmann_crowding_2012-1,
  title = {Crowding {{Deficits}} in the {{Visual Periphery}} of {{Schizophrenia Patients}}},
  volume = {7},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0045884},
  abstract = {Accumulating evidence suggests that basic visual information processing is impaired in schizophrenia. However, deficits in peripheral vision remain largely unexplored. Here we hypothesized that sensory processing of information in the visual periphery would be impaired in schizophrenia patients and, as a result, crowding \textendash{} the breakdown in target recognition that occurs in cluttered visual environments \textendash{} would be stronger. Therefore, we assessed visual crowding in the peripheral vision of schizophrenia patients and healthy controls. Subjects were asked to identify a target letter that was surrounded by distracter letters of similar appearance. Targets and distracters were displayed at 8$^\circ$ and 10$^\circ$ of visual angle from the fixation point (eccentricity), and target-distracter spacing was 2$^\circ$, 3$^\circ$, 4$^\circ$, 5$^\circ$, 6$^\circ$, 7$^\circ$ or 8$^\circ$ of visual angle. Eccentricity and target-distracter spacing were randomly varied. Accuracy was defined as the proportion of correctly identified targets. Critical spacing was defined as the spacing at which target identification accuracy began to deteriorate, and was assessed at viewing eccentricities of 8$^\circ$ and 10$^\circ$. Schizophrenia patients were less accurate and showed a larger critical spacing than healthy individuals. These results indicate that crowding is stronger and sensory processing of information in the visual periphery is impaired in schizophrenia. This is in line with previous reports of preferential magnocellular dysfunction in schizophrenia. Thus, deficits in peripheral vision may account for perceptual alterations and contribute to cognitive dysfunction in schizophrenia.},
  timestamp = {2017-03-24T21:26:08Z},
  number = {9},
  urldate = {2017-03-24},
  journal = {PLOS ONE},
  author = {Kraehenmann, Rainer and Vollenweider, Franz X. and Seifritz, Erich and Kometer, Michael},
  year = {26-Sep-2012},
  keywords = {Attention,eye movements,Face recognition,perception,schizophrenia,Sensory perception,vision,Visual impairments},
  pages = {e45884},
  file = {Kraehenmann et al_2012_Crowding Deficits in the Visual Periphery of Schizophrenia Patients.pdf:/Users/jan/Documents/Zotero/storage/PB3TFEN4/Kraehenmann et al_2012_Crowding Deficits in the Visual Periphery of Schizophrenia Patients.pdf:application/pdf;Snapshot:/Users/jan/Documents/Zotero/storage/PUFS4SD6/article.html:text/html},
  groups = {Perception,Perception,Perception}
}

@article{yang_visual_2013,
  title = {Visual {{Context Processing}} in {{Schizophrenia}}},
  volume = {1},
  issn = {2167-7026},
  doi = {10.1177/2167702612464618},
  abstract = {Abnormal perceptual experiences are central to schizophrenia, but the nature of these anomalies remains undetermined. We investigated contextual processing abnormalities across a comprehensive set of visual tasks. For perception of luminance, size, contrast, orientation, and motion, we quantified the degree to which the surrounding visual context altered a center stimulus's appearance. Healthy participants showed robust contextual effects across all tasks, as evidenced by pronounced misperceptions of center stimuli. Schizophrenia patients exhibited intact contextual modulations of luminance and size but showed weakened contextual modulations of contrast, performing more accurately than controls. Strong motion and orientation context effects correlated with worse symptoms and social functioning. Importantly, the overall strength of contextual modulation across tasks did not differ between controls and schizophrenia patients. In addition, performance measures across contextual tasks were uncorrelated, implying discrete underlying processes. These findings reveal that abnormal contextual modulation in schizophrenia is selective, arguing against the proposed unitary contextual processing dysfunction.},
  language = {en},
  timestamp = {2017-03-24T21:27:35Z},
  number = {1},
  urldate = {2017-03-24},
  journal = {Clinical Psychological Science},
  author = {Yang, Eunice and Tadin, Duje and Glasser, Davis M. and Hong, Sang Wook and Blake, Randolph and Park, Sohee},
  month = jan,
  year = {2013},
  pages = {5--15},
  file = {Yang et al_2013_Visual Context Processing in Schizophrenia.pdf:/Users/jan/Documents/Zotero/storage/8VF6TPNK/Yang et al_2013_Visual Context Processing in Schizophrenia.pdf:application/pdf},
  groups = {Perception,Perception,Perception,JoN Journal Club,JoN Journal Club}
}

@article{butler_early-stage_2005,
  title = {Early-{{Stage Visual Processing}} and {{Cortical Amplification Deficits}} in {{Schizophrenia}}},
  volume = {62},
  issn = {0003-990X},
  doi = {10.1001/archpsyc.62.5.495},
  abstract = {$<$h3$>$Background$<$/h3$>$$<$p$>$Patients with schizophrenia show deficits in early-stage visual processing, potentially reflecting dysfunction of the magnocellular visual pathway. The magnocellular system operates normally in a nonlinear amplification mode mediated by glutamatergic (\emph{N}-methyl-D-aspartate) receptors. Investigating magnocellular dysfunction in schizophrenia therefore permits evaluation of underlying etiologic hypotheses.$<$/p$>$$<$h3$>$Objectives$<$/h3$>$$<$p$>$To evaluate magnocellular dysfunction in schizophrenia, relative to known neurochemical and neuroanatomical substrates, and to examine relationships between electrophysiological and behavioral measures of visual pathway dysfunction and relationships with higher cognitive deficits.$<$/p$>$$<$h3$>$Design, Setting, and Participants$<$/h3$>$$<$p$>$Between-group study at an inpatient state psychiatric hospital and outpatient county psychiatric facilities. Thirty-three patients met\emph{DSM-IV}criteria for schizophrenia or schizoaffective disorder, and 21 nonpsychiatric volunteers of similar ages composed the control group.$<$/p$>$$<$h3$>$Main Outcome Measures$<$/h3$>$$<$p$>$(1) Magnocellular and parvocellular evoked potentials, analyzed using nonlinear (Michaelis-Menten) and linear contrast gain approaches; (2) behavioral contrast sensitivity measures; (3) white matter integrity; (4) visual and nonvisual neuropsychological measures, and (5) clinical symptom and community functioning measures.$<$/p$>$$<$h3$>$Results$<$/h3$>$$<$p$>$Patients generated evoked potentials that were significantly reduced in response to magnocellular-biased, but not parvocellular-biased, stimuli (\emph{P} = .001). Michaelis-Menten analyses demonstrated reduced contrast gain of the magnocellular system (\emph{P} = .001). Patients showed decreased contrast sensitivity to magnocellular-biased stimuli (\emph{P}\&lt;.001). Evoked potential deficits were significantly related to decreased white matter integrity in the optic radiations (\emph{P}\&lt;.03). Evoked potential deficits predicted impaired contrast sensitivity (\emph{P} = .002), which was in turn related to deficits in complex visual processing (\emph{P}$\leq$.04). Both evoked potential (\emph{P}$\leq$.04) and contrast sensitivity (\emph{P} = .01) measures significantly predicted community functioning.$<$/p$>$$<$h3$>$Conclusions$<$/h3$>$$<$p$>$These findings confirm the existence of early-stage visual processing dysfunction in schizophrenia and provide the first evidence that such deficits are due to decreased nonlinear signal amplification, consistent with glutamatergic theories. Neuroimaging studies support the hypothesis of dysfunction within low-level visual pathways involving thalamocortical radiations. Deficits in early-stage visual processing significantly predict higher cognitive deficits.$<$/p$>$},
  timestamp = {2017-03-24T21:33:14Z},
  number = {5},
  urldate = {2017-03-24},
  journal = {Archives of General Psychiatry},
  author = {Butler, Pamela D. and Zemon, Vance and Schechter, Isaac and Saperstein, Alice M. and Hoptman, Matthew J. and Lim, Kelvin O. and Revheim, Nadine and Silipo, Gail and Javitt, Daniel C.},
  month = may,
  year = {2005},
  pages = {495--504},
  file = {Butler et al_2005_Early-Stage Visual Processing and Cortical Amplification Deficits in.pdf:/Users/jan/Documents/Zotero/storage/RHKK4ITM/Butler et al_2005_Early-Stage Visual Processing and Cortical Amplification Deficits in.pdf:application/pdf;Snapshot:/Users/jan/Documents/Zotero/storage/KUW266KU/208560.html:text/html},
  groups = {Perception,Perception,Perception}
}

