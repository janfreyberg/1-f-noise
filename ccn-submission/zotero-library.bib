  pages = {22},
  file = {Freeman, Pelli (2007) - An escape from crowding.pdf:/Users/jan/Documents/Zotero/storage/H25H8WG3/Freeman, Pelli (2007) - An escape from crowding.pdf:application/pdf;Snapshot:/Users/jan/Documents/Zotero/storage/EKHWNJMX/22.html:text/html},
  groups = {Crowding,Crowding \& Attention,Crowding,Crowding \& Attention,Crowding,Crowding \& Attention,Crowding \& Attention}
