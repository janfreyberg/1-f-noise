  journal = {Journal of Magnetic Resonance Imaging},
  author = {O'Gorman, Ruth L. and Michels, Lars and Edden, Richard A. and Murdoch, James B. and Martin, Ernst},
  month = may,
  year = {2011},
  keywords = {gaba,glutamate,MR spectroscopy,reproducibility of results,sex},
  pages = {1262--1267},
  file = {O'Gorman et al (2011) In vivo detection of GABA and glutamate with MEGA-PRESS.pdf:/Users/jan/Documents/Zotero/storage/FPUTDK54/O'Gorman et al (2011) In vivo detection of GABA and glutamate with MEGA-PRESS.pdf:application/pdf;Snapshot:/Users/jan/Documents/Zotero/storage/HW4KGIMI/abstract.html:text/html},
