  month = apr,
  year = {2012},
  keywords = {binocular rivalry,Onset binocular rivalry,onset rivalry},
  pages = {942--953},
  file = {Hupé_Pressnitzer_2012_The initial phase of auditory and visual scene analysis.pdf:/Users/jan/Documents/Zotero/storage/TZ9XG6ES/Hupé_Pressnitzer_2012_The initial phase of auditory and visual scene analysis.pdf:application/pdf;Snapshot:/Users/jan/Documents/Zotero/storage/29XCTCF5/942.html:text/html},
