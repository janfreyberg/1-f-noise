  author = {Skerswetat, Jan and Formankiewicz, Monika A. and Waugh, Sarah J.},
  month = apr,
  year = {2016},
  keywords = {binocular,binocular rivalry,contrast,Mixed percepts,Second-order},
  pages = {10--22},
  file = {Skerswetat et al_2016_Very few exclusive percepts for contrast-modulated stimuli during binocular.pdf:/Users/jan/Documents/Zotero/storage/MRM7UVFN/Skerswetat et al_2016_Very few exclusive percepts for contrast-modulated stimuli during binocular.pdf:application/pdf;ScienceDirect Snapshot:/Users/jan/Documents/Zotero/storage/IC2E86J7/S0042698916000146.html:text/html},
