  pages = {37--54},
  file = {Shpiro et al_2009_Balance between noise and adaptation in competition models of perceptual.pdf:/Users/jan/Documents/Zotero/storage/25I3H2EH/Shpiro et al_2009_Balance between noise and adaptation in competition models of perceptual.pdf:application/pdf;Snapshot:/Users/jan/Documents/Zotero/storage/AVJTTDD3/s10827-008-0125-3.html:text/html},
