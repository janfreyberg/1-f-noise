  journal = {Journal of vision},
  author = {Hancock, Sarah and Whitney, David and Andrews, Timothy J.},
  month = jan,
  year = {2008},
  pages = {3.1--3.9},
  file = {Hancock et al_2008_The initial interactions underlying binocular rivalry require visual awareness.pdf:/Users/jan/Documents/Zotero/storage/7MZ4BFKB/Hancock et al_2008_The initial interactions underlying binocular rivalry require visual awareness.pdf:application/pdf},
  groups = {Onset \& Intermittent Rivalry,Onset \& Intermittent Rivalry,Onset \& Intermittent Rivalry,Binocular Rivalry,Binocular Rivalry},
  pmid = {18318606},
  pmcid = {PMC2696403}
}

@article{bueti_different_2007,
  title = {Different {{Brain Circuits Underlie Motor}} and {{Perceptual Representations}} of {{Temporal Intervals}}},
  volume = {20},
  issn = {0898-929X},
  doi = {10.1162/jocn.2008.20017},
  abstract = {In everyday life, temporal information is used for both perception and action, but whether these two functions reflect the operation of similar or different neural circuits is unclear. We used functional magnetic resonance imaging to investigate the neural correlates of processing temporal information when either a motor or a perceptual representation is used. Participants viewed two identical sequences of visual stimuli and used the information differently to perform either a temporal reproduction or a temporal estimation task. By comparing brain activity evoked by these tasks and control conditions, we explored commonalities and differences in brain areas involved in reproduction and estimation of temporal intervals. The basal ganglia and the cerebellum were commonly active in both temporal tasks, consistent with suggestions that perception and production of time are subserved by the same mechanisms. However, only in the reproduction task was activity observed in a wider cortical network including the right pre-SMA, left middle frontal gyrus, left premotor cortex, with a more reliable activity in the right inferior parietal cortex, left fusiform gyrus, and the right extrastriate visual area V5/MT. Our findings point to a role for the parietal cortex as an interface between sensory and motor processes and suggest that it may be a key node in translation of temporal information into action. Furthermore, we discuss the potential importance of the extrastriate cortex in processing visual time in the context of recent findings.},
  timestamp = {2016-12-12T16:40:03Z},
  number = {2},
  urldate = {2016-12-12},
  journal = {Journal of Cognitive Neuroscience},
  author = {Bueti, Domenica and Walsh, Vincent and Frith, Chris and Rees, Geraint},
  month = oct,
  year = {2007},
  pages = {204--214},
  file = {Bueti et al_2007_Different Brain Circuits Underlie Motor and Perceptual Representations of.pdf:/Users/jan/Documents/Zotero/storage/5MED8S84/Bueti et al_2007_Different Brain Circuits Underlie Motor and Perceptual Representations of.pdf:application/pdf;Journal of Cognitive Neuroscience Snapshot:/Users/jan/Documents/Zotero/storage/GIKIGK9Q/jocn.2008.html:text/html},
