  file = {Frässle et al (2014) - Binocular Rivalry.pdf:/Users/jan/Documents/Zotero/storage/6QCC7W8D/Frässle et al (2014) - Binocular Rivalry.pdf:application/pdf;Snapshot:/Users/jan/Documents/Zotero/storage/C2NEFT7G/1738.html:text/html},
