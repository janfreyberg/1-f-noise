  journal = {The Journal of Neuroscience},
  author = {Corcoran, Kevin A and Quirk, Gregory J},
  month = jan,
  year = {2007},
  keywords = {predator,prefrontal cortex,rat,tetrodotoxin},
  pages = {840--844}
}

