  month = jan,
  year = {2004},
  note = {00042},
  keywords = {coherence,erp,schizophrenia,Steady-state,visual},
  pages = {121--129},
  file = {Clementz et al (2004) Aberrant brain dynamics in schizophrenia.pdf:/Users/jan/Documents/Zotero/storage/JT8CTMEP/Clementz et al (2004) Aberrant brain dynamics in schizophrenia.pdf:application/pdf;ScienceDirect Snapshot:/Users/jan/Documents/Zotero/storage/E7EWGD4K/S0926641003002416.html:text/html},
  groups = {SS VEP,SS VEP,SS VEP,SS VEP}
