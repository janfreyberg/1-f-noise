  month = may,
  year = {2016},
  keywords = {preprint},
  pages = {051912},
  file = {Burgess et al_2016_High-yield methods for accurate two-alternative visual psychophysics in.pdf:/Users/jan/Documents/Zotero/storage/MUEQCBKB/Burgess et al_2016_High-yield methods for accurate two-alternative visual psychophysics in.pdf:application/pdf;Snapshot:/Users/jan/Documents/Zotero/storage/C29894IU/051912.full.html:text/html},
