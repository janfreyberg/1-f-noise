@incollection{plaisted_reduced_2001,
  title = {Reduced Generalization in Autism: {{An}} Alternative to Weak Central Coherence.},
  abstract = {Argues that evidence is highly suggestive of a causal link between the perceptual and attentional abnormalities in individuals with autism and their deficits in social processing. The readoption of the parsimonious position that many aspects of autism stem from one underlying deficit is suggested. It is argued that the question then becomes whether this deficit is a weakening of central-coherence mechanisms or whether weak central coherence "effects" may be better explained by alternative mechanisms. This questions is answered by reviewing studies that have assessed the weak central coherence hypothesis at the level of perception and selective attention. It is argued that there is an alternative explanation of the perceptual and attentional abnormalities in autism to weak central coherence that pivots on the notion that individuals with autism are unable to draw pieces of information together because of an inability to recognize the similarities between stimuli or situations. Ways in which a reduced ability to process similarity at the perceptual and attentional level (of the kind observed in autism) can lead to abnormalities at the conceptual level are offered.},
  timestamp = {2012-04-22T20:15:38Z},
  booktitle = {The Development of Autism: {{Perspectives}} from Theory and Research.},
