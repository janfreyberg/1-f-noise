  keywords = {autism,autism spectrum disorder,Motion Perception,Pattern Recognition,Space Perception,Visual Perception,Weak Central Coherence},
  pages = {1044--53},
  file = {Pellicano et al (2005) Abnormal global processing along the dorsal visual pathway in autism.pdf:/Users/jan/Documents/Zotero/storage/DKNTDKRT/Pellicano et al (2005) Abnormal global processing along the dorsal visual pathway in autism.pdf:application/pdf;ScienceDirect Snapshot:/Users/jan/Documents/Zotero/storage/R58EQ4S6/S0028393204002702.html:text/html},
  groups = {Autism - Motion Perception,Autism - Vision,Autism - Motion Perception,Autism - Vision,Autism - Motion Perception,Autism - Vision}
