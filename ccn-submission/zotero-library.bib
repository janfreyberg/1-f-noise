@misc{qian_schizophrenia_2012,
  address = {New Orleans},
  type = {Poster},
  title = {Schizophrenia and Autism as Opposite Tuning-Function Disorders: {{A}} Theory},
  abstract = {Schizophrenia and autism are each associated with a large number of genetic and environmental factors, and no single factor controls either disorder. Yet, each disorder clusters around its characteristic set of behaviors (albeit with individual variations), suggesting that the actions of diverse factors for each disorder may converge to induce similar functional consequences. We propose a framework in which the converging point is the tuning functions of neuronal responses to various stimulus features. Specifically, we hypothesize that schizophrenic and autistic brains have broader- and narrower-than-typical tuning functions, respectively, possibly produced by a decrease and increase of nonlinear gains in neuronal responses (Butler et al., 2005).
To perform daily tasks such as face identification or language comprehension, the brain must learn to map inputs (e.g., retinal images) to outputs (e.g., face identity) by extracting relevant, complex regularities from inputs. Computationally, the brain could approximate input-output mappings by nonlinearly combining neuronal responses along processing hierarchies. Different tasks or mappings entail different underlying regularities that are best learned by neurons with different tuning functions. If typical brains have optimized tuning functions to learn socially important tasks, then suboptimal tuning may impair social learning and produce behavioral deficits. Specifically, narrower-than optimal tuning functions bias a system toward rote memorization of experiences without extracting underlying regularities for generalization, leading to, among other things, rigid behaviors, as in autism (Qian \& Lipkin, 2011). Conversely, broader-than-optimal tuning functions impede memorization of multiple items, as in schizophrenia, because of increased overlapping or interference among responses to different items. Thus, broader tuning functions represent experiences less faithfully and encourage over-interpolation/generalization of actual experiences, which could generate illusory ones. Finally, narrower and broader tuning functions are consistent with lack of imagination in autism and loose association in schizophrenia, respectively.
As a prediction, we simulated orientation tilt aftereffect for a range of tuning width and found that narrow (autism) and broad (schizophrenia) tuning produces aftereffects over correspondingly narrow and broad ranges of differences between adapting and test stimuli. This is consistent with Rokem et al.'s (2011) schizophrenia study. Our theory suggests that drugs affecting tuning in opposite ways might help reduce symptoms of these disorders.},
  language = {English},
  timestamp = {2013-06-25T11:26:22Z},
  urldate = {2013-06-25},
  author = {Qian, N and Lipkin, RM and Javitt, DC},
  month = oct,
  year = {2012},
  file = {Qian et al (2012) - Schizophrenia and autism as opposite tuning-function disorders.pdf:/Users/jan/Documents/Zotero/storage/XSJPNV8D/Qian et al (2012) - Schizophrenia and autism as opposite tuning-function disorders.pdf:application/pdf},
  groups = {Autism - Vision,Autism - Vision,Autism - Vision}
}

