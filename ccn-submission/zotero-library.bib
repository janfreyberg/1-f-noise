@article{kwon_gamma_1999,
  title = {Gamma {{Frequency}}\textendash{}{{Range Abnormalities}} to {{Auditory Stimulation}} in {{Schizophrenia}}},
  volume = {56},
  issn = {0003-990X},
  doi = {10.1001/archpsyc.56.11.1001},
  abstract = {$<$h3$>$Background$<$/h3$>$$<$p$>$Basic science studies at the neuronal systems level have indicated that gamma-range (30-50 Hz) neural synchronization may be a key mechanism of information processing in neural networks, reflecting integration of various features of an object. Furthermore, gamma-range synchronization is thought to depend on the glutamatergically mediated interplay between excitatory projection neurons and inhibitory neurons utilizing $\gamma$-aminobutyric acid (GABA), which postmortem studies suggest may be abnormal in schizophrenia. We therefore tested whether auditory neural networks in patients with schizophrenia could support gamma-range synchronization.$<$/p$>$$<$h3$>$Methods$<$/h3$>$$<$p$>$Synchronization of the electroencephalogram (EEG) to different rates (20-40 Hz) of auditory stimulation was recorded from 15 patients with schizophrenia and 15 sex-, age-, and handedness-matched control subjects. The EEG power at each stimulation frequency was compared between groups. The time course of the phase relationship between each stimulus and EEG peak was also evaluated for gamma-range (40 Hz) stimulation.$<$/p$>$$<$h3$>$Results$<$/h3$>$$<$p$>$Schizophrenic patients showed reduced EEG power at 40 Hz, but not at lower frequencies of stimulation. In addition, schizophrenic patients showed delayed onset of phase synchronization and delayed desynchronization to the click train.$<$/p$>$$<$h3$>$Conclusions$<$/h3$>$$<$p$>$These data provide new information on selective deficits in early-stage sensory processing in schizophrenia, a failure to support the entrainment of intrinsic gamma-frequency oscillators. The reduced EEG power at 40 Hz in schizophrenic patients may reflect a dysfunction of the recurrent inhibitory drive on auditory neural networks.$<$/p$>$},
  timestamp = {2017-03-24T20:07:33Z},
  number = {11},
  urldate = {2017-03-24},
  journal = {Archives of General Psychiatry},
  author = {Kwon, Jun Soo and O'Donnell, Brian F. and Wallenstein, Gene V. and Greene, Robert W. and Hirayasu, Yoshio and Nestor, Paul G. and Hasselmo, Michael E. and Potts, Geoffrey F. and Shenton, Martha E. and McCarley, Robert W.},
  month = nov,
  year = {1999},
  pages = {1001--1005},
  file = {Kwon et al_1999_Gamma Frequency–Range Abnormalities to Auditory Stimulation in Schizophrenia.pdf:/Users/jan/Documents/Zotero/storage/TMW4MET2/Kwon et al_1999_Gamma Frequency–Range Abnormalities to Auditory Stimulation in Schizophrenia.pdf:application/pdf;Snapshot:/Users/jan/Documents/Zotero/storage/3JUPVVB8/205458.html:text/html},
  groups = {Gamma,Gamma,Gamma}
}

@article{light_gamma_2006,
  title = {Gamma {{Band Oscillations Reveal Neural Network Cortical Coherence Dysfunction}} in {{Schizophrenia Patients}}},
  volume = {60},
  issn = {0006-3223},
  doi = {10.1016/j.biopsych.2006.03.055},
  abstract = {Background
Gamma band activity has been associated with many sensory and cognitive functions, and is important for cortico-cortical transmission and the integration of information across neural networks. The aims of the present study were to determine if schizophrenia patients have deficits in the generation and maintenance of coherent, synchronized oscillations in response to steady-state stimulation, and to examine the clinical and cognitive correlates of the electroencephalography (EEG) oscillatory dynamics.
Methods
Schizophrenia patients (n = 100) and nonpsychiatric subjects (n = 80) underwent auditory steady-state event-related potential testing. Click trains varying in rate of stimulation (20, 30, and 40 Hz) were presented; EEG-evoked power and intertrial phase synchronization were obtained in response to each stimulation frequency. Subjects also underwent clinical and neurocognitive assessments.
Results
Patients had reductions in both evoked power and phase synchronization in response to 30- and 40-Hz stimulation but normal responsivity to 20-Hz stimulation. Maximal deficits were detected in response to 40-Hz stimulation. A modest association of reduced working memory performance and 40-Hz intertrial phase synchronization was present in schizophrenia patients (r = .32, p \&lt;.01).
Conclusions
Schizophrenia patients have frequency-specific deficits in the generation and maintenance of coherent gamma-range oscillations, reflecting a fundamental degradation of basic integrated neural network activity.},
  timestamp = {2017-03-24T20:08:19Z},
  number = {11},
  urldate = {2017-03-24},
  journal = {Biological Psychiatry},
  author = {Light, Gregory A. and Hsu, Jung Lung and Hsieh, Ming H. and Meyer-Gomes, Katrin and Sprock, Joyce and Swerdlow, Neal R. and Braff, David L.},
  month = dec,
  year = {2006},
  keywords = {cognition,coherence,EEG,event-related potential,evoked power,Gamma band,induced oscillations,Oscillations,schizophrenia,working memory},
  pages = {1231--1240},
  file = {Light et al_2006_Gamma Band Oscillations Reveal Neural Network Cortical Coherence Dysfunction in.pdf:/Users/jan/Documents/Zotero/storage/RBJ7ARCN/Light et al_2006_Gamma Band Oscillations Reveal Neural Network Cortical Coherence Dysfunction in.pdf:application/pdf;ScienceDirect Snapshot:/Users/jan/Documents/Zotero/storage/PKKUIEQ4/S0006322306005361.html:text/html},
  groups = {Gamma,Gamma,Gamma}
}

@article{spencer_sensory-evoked_2008,
  series = {Neural Network Dysfunction in Schizophrenia: From Cells, to Networks, to Behavior},
  title = {Sensory-{{Evoked Gamma Oscillations}} in {{Chronic Schizophrenia}}},
  volume = {63},
  issn = {0006-3223},
  doi = {10.1016/j.biopsych.2007.10.017},
  abstract = {Background
The early visual-evoked gamma oscillation (VGO) elicited by Gestalt stimuli is reduced in schizophrenia patients compared with healthy individuals, but it is unknown whether this effect is specific to these particular stimuli and task. In contrast, the early auditory-evoked gamma oscillation (AGO) was reported to be unaffected in a sample of unmedicated, mostly first-episode schizophrenics, but it is unknown whether this oscillation is abnormal in chronic, medicated patients. We investigated these issues by examining the VGO and AGO in chronic schizophrenic (SZ) and matched healthy control (HC) subjects.
Methods
Subjects (21 HC, 23 SZ) performed visual and auditory oddball tasks. Visual stimuli were letters, and auditory stimuli were simple tones. Event-related spectral measures (phase locking factor and evoked power) were computed on Morlet wavelet-transformed single epochs from the standard trials.
Results
VGO phase locking at occipital electrodes was reduced in SZ compared with HC. In contrast, AGO phase locking and evoked power did not differ between groups.
Conclusions
The VGO deficit may be a general phenomenon in schizophrenia, whereas the AGO evoked by simple tone stimuli does not appear to be abnormal in chronic, medicated schizophrenia patients.},
  timestamp = {2017-03-24T20:08:26Z},
  number = {8},
  urldate = {2017-03-24},
  journal = {Biological Psychiatry},
  author = {Spencer, Kevin M. and Niznikiewicz, Margaret A. and Shenton, Martha E. and McCarley, Robert W.},
  month = apr,
  year = {2008},
  keywords = {EEG,Evoked Potentials,Gamma oscillation,schizophrenia},
  pages = {744--747},
  file = {ScienceDirect Snapshot:/Users/jan/Documents/Zotero/storage/7EIREHBX/S0006322307010372.html:text/html},
  groups = {Gamma,Gamma,Gamma}
}

@article{uhlhaas_abnormal_2010,
  title = {Abnormal Neural Oscillations and Synchrony in Schizophrenia},
  volume = {11},
  copyright = {\textcopyright{} 2010 Nature Publishing Group},
  issn = {1471-003X},
  doi = {10.1038/nrn2774},
  abstract = {Converging evidence from electrophysiological, physiological and anatomical studies suggests that abnormalities in the synchronized oscillatory activity of neurons may have a central role in the pathophysiology of schizophrenia. Neural oscillations are a fundamental mechanism for the establishment of precise temporal relationships between neuronal responses that are in turn relevant for memory, perception and consciousness. In patients with schizophrenia, the synchronization of beta- and gamma-band activity is abnormal, suggesting a crucial role for dysfunctional oscillations in the generation of the cognitive deficits and other symptoms of the disorder. Dysfunctional oscillations may arise owing to anomalies in the brain's rhythm-generating networks of GABA ($\gamma$-aminobutyric acid) interneurons and in cortico-cortical connections.},
  language = {en},
  timestamp = {2017-03-24T20:53:50Z},
  number = {2},
  urldate = {2017-03-24},
  journal = {Nature Reviews Neuroscience},
  author = {Uhlhaas, Peter J. and Singer, Wolf},
  month = feb,
  year = {2010},
  pages = {100--113},
  file = {Uhlhaas_Singer_2010_Abnormal neural oscillations and synchrony in schizophrenia.pdf:/Users/jan/Documents/Zotero/storage/TN8AZ969/Uhlhaas_Singer_2010_Abnormal neural oscillations and synchrony in schizophrenia.pdf:application/pdf;Snapshot:/Users/jan/Documents/Zotero/storage/2VN3IE4Z/nrn2774.html:text/html}
}

@article{robol_reduced_2013,
  title = {Reduced {{Crowding}} and {{Poor Contour Detection}} in {{Schizophrenia Are Consistent}} with {{Weak Surround Inhibition}}},
  volume = {8},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0060951},
  abstract = {Background Detection of visual contours (strings of small oriented elements) is markedly poor in schizophrenia. This has previously been attributed to an inability to group local information across space into a global percept. Here, we show that this failure actually originates from a combination of poor encoding of local orientation and abnormal processing of visual context.   Methods We measured the ability of observers with schizophrenia to localise contours embedded in backgrounds of differently oriented elements (either randomly oriented, near-parallel or near-perpendicular to the contour). In addition, we measured patients' ability to process local orientation information (i.e., report the orientation of an individual element) for both isolated and crowded elements (i.e., presented with nearby distractors).   Results While patients are poor at detecting contours amongst randomly oriented elements, they are proportionally less disrupted (compared to unaffected controls) when contour and surrounding elements have similar orientations (near-parallel condition). In addition, patients are poor at reporting the orientation of an individual element but, again, are less prone to interference from nearby distractors, a phenomenon known as visual crowding.   Conclusions We suggest that patients' poor performance at contour perception arises not as a consequence of an ``integration deficit'' but from a combination of reduced sensitivity to local orientation and abnormalities in contextual processing. We propose that this is a consequence of abnormal gain control, a phenomenon that has been implicated in orientation-selectivity as well as surround suppression.},
  timestamp = {2017-03-24T21:10:17Z},
  number = {4},
  urldate = {2017-03-24},
  journal = {PLOS ONE},
  author = {Robol, Valentina and Tibber, Marc S. and Anderson, Elaine J. and Bobin, Tracy and Carlin, Patricia and Shergill, Sukhwinder S. and Dakin, Steven C.},
  year = {09-Apr-2013},
  keywords = {Analysis of variance,cognition,Gamma-aminobutyric acid,Neuronal tuning,Neurons,psychophysics,schizophrenia,vision},
  pages = {e60951},
  file = {Robol et al_2013_Reduced Crowding and Poor Contour Detection in Schizophrenia Are Consistent.pdf:/Users/jan/Documents/Zotero/storage/6TXQSRAU/Robol et al_2013_Reduced Crowding and Poor Contour Detection in Schizophrenia Are Consistent.pdf:application/pdf;Snapshot:/Users/jan/Documents/Zotero/storage/NSUPFGK6/article.html:text/html},
  groups = {Perception,Perception,Perception}
}

@article{silverstein_perceptual_2000,
  title = {Perceptual Organization, the Disorganization Syndrome, and Context Processing in Chronic Schizophrenia},
  volume = {43},
  issn = {0920-9964},
  doi = {10.1016/S0920-9964(99)00180-2},
  abstract = {Schizophrenia patients' perceptual organization abilities were assessed with a psychophysically well-controlled measure of contour integration. Compared with psychiatric and staff controls, schizophrenia patients were less able to detect contours comprising Gabor elements as the detection of these contours relied increasingly on long-range spatial interactions. Impaired task performance was also found to correlate significantly with higher levels of disorganized symptomatology. These data provide further evidence for impaired perceptual grouping in schizophrenia. In addition, the findings support the hypothesis that a common cortical processing algorithm involving contextual coordination is impaired in schizophrenia, leading to reduced binding of object features in vision, and reduced contextual disambiguation of linguistic information during thought and speech.},
  timestamp = {2017-03-24T21:13:03Z},
  number = {1},
  urldate = {2017-03-24},
  journal = {Schizophrenia Research},
  author = {Silverstein, Steven M. and Kov{\'a}cs, Ilona and Corry, Rodney and Valone, Carolyn},
  month = may,
  year = {2000},
  keywords = {Context processing,Contour integration,Disorganization syndrome,perceptual organization,visual information processing},
  pages = {11--20},
  file = {Silverstein et al_2000_Perceptual organization, the disorganization syndrome, and context processing.pdf:/Users/jan/Documents/Zotero/storage/CTIRP43F/Silverstein et al_2000_Perceptual organization, the disorganization syndrome, and context processing.pdf:application/pdf;ScienceDirect Snapshot:/Users/jan/Documents/Zotero/storage/HS275VA4/S0920996499001802.html:text/html},
