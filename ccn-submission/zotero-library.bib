  author = {{Greta Vilidaite} and {Natasha Gutmanis} and {Samantha Harris} and {Rebecca Kitching} and {Hollie Melton} and {Roseanna Norman} and {Christina Scott} and {Anika Smith} and {Kirstie Wailes-Newson} and {Daniel Baker}},
  year = {2015},
  file = {Greta Vilidaite et al_2015_Estimating inter-observer variability in GABA-ergic suppression.pdf:/Users/jan/Documents/Zotero/storage/QSTC85MW/Greta Vilidaite et al_2015_Estimating inter-observer variability in GABA-ergic suppression.pdf:application/pdf},
  groups = {EEG and GABA,EEG and GABA}
}

@article{solomon_moving_2014,
