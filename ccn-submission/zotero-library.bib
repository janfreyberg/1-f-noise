  file = {Freeman et al (2003) - Top-Down Modulation of Lateral Interactions in Early Vision.pdf:/Users/jan/Documents/Zotero/storage/5XMTCKHC/Freeman et al (2003) - Top-Down Modulation of Lateral Interactions in Early Vision.pdf:application/pdf;ScienceDirect Snapshot:/Users/jan/Documents/Zotero/storage/W3PC75TT/S0960982203003336.html:text/html},
