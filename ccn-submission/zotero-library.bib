  keywords = {animal models,Nerve Net: cytology,Nerve Net: physiology,neural inhibition,Neural Inhibition: physiology,thalamus,Thalamus: physiology,vision,Visual Cortex: cytology,Visual Pathways: cytology},
  pages = {63--9},
  groups = {Visual Perception,Visual Perception,Visual Perception}
}

